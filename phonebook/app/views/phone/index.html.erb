<h1>Phone#index</h1>
<table border="0" >
  <tr>
   <td>Name</td><td>Phone Number</td><td>Birthday</td>
  </tr>
<% @phones.each do |p| %>
  <tr>
   <td><%=p.name %></td><td><%=p.phone%></td><td><%=p.birthday%></td>
   <td>
	<%= link_to "edit" , :action =>"modify" , :id => p%> /
	<%= link_to "del" , :action => "del", :id => p%>
   </td>
  </tr>
<% end%>
</table>
<p><%= link_to "add new phone number" , phone_newphone_path% ></p>
